{
  "comments": {
    "lineComment": ". "
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*\\.(def|func|comment|raw|code)\\s*$",
      "end": "^\\s*\\.(end|__RAW__)\\s*$"
    }
  },
  "wordPattern": "\\$[a-zA-Z][a-zA-Z0-9_]*(\\.[a-zA-Z][a-zA-Z0-9_]*)*|\\$\\$[a-zA-Z][a-zA-Z0-9_]*(\\.[a-zA-Z][a-zA-Z0-9_]*)*|[a-zA-Z_][a-zA-Z0-9_]*",
  "indentationRules": {
    "increaseIndentPattern": "^\\s*\\.(def|func|comment|raw|code)\\s*$",
    "decreaseIndentPattern": "^\\s*\\.(end|__RAW__)\\s*$"
  }
}
